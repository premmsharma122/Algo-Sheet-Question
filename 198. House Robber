class Solution {
    public int help(int arr[], int idx, int dp[]){
        if(idx >= arr.length) return 0;
        if(dp[idx] != -1) return dp[idx];
        int ans1 =  arr[idx] + help(arr,idx+2,dp);
        int ans2 = help(arr,idx+1,dp);
        dp[idx] =  Math.max(ans1, ans2);
        return dp[idx];
    }
    public int rob(int[] nums) {
        int dp[] = new int[nums.length];
        Arrays.fill(dp,-1);
        return Math.max(help(nums,0,dp), help(nums,1,dp));
    }
}
